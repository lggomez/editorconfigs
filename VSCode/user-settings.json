{
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Monokai",
    "go.enableCodeLens": {
        "references": false,
        "runtest": true,
    },
    "go.delveConfig": {
        "useApiV1": false,
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 3,
            "maxStringLen": 64,
            "maxArrayValues": 200,
            "maxStructFields": -1
        }
    },
    "go.testTimeout": "60s",
    "go.autocompleteUnimportedPackages": false,
    "go.testFlags": [
        "-v"
    ],
    "git.decorations.enabled": true,
    "editor.lightbulb.enabled": true,
    "go.formatTool": "gofmt",
    "go.lintOnSave": "package",
    "go.lintTool": "gometalinter",
    //"go.lintTool": "golint",
// LANGUAGE SERVER START
    "go.useLanguageServer": true,
    "go.alternateTools": {
        "go-langserver": "bingo"
    },
    "go.languageServerFlags": [
        "-enhance-signature-help",
        "-format-style=gofmt",
        "-cache-style=always",
        "-diagnostics-style=onsave",
        "-trace",
    ],
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true
    },
// LANGUAGE SERVER END
    "files.associations": {
        "Gopkg.lock": "toml"
    },
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/tmp": true,
        "**/vendor": true,
    },
    "workbench.statusBar.feedback.visible": false,
    // "launch": {
    //     "version": "0.2.0",
    //     "configurations": [
    //         {
    //             "name": "Launch (default)",
    //             "type": "go",
    //             "request": "launch",
    //             "mode": "debug",
    //             "remotePath": "",
    //             "port": 2345,
    //             "host": "127.0.0.1",
    //             "program": "${fileDirname}",
    //             "env": {},
    //             "args": [],
    //             "showLog": true,
    //             "useApiV1": false,
    //             "dlvLoadConfig": {
    //                 "followPointers": true,
    //                 "maxVariableRecurse": 1,
    //                 "maxStringLen": 5,
    //                 "maxArrayValues": 128,
    //                 "maxStructFields": -1
    //             }
    //         }
    //     ]
    // },
    "terminal.integrated.rendererType": "dom",
    "editor.accessibilitySupport": "off",
    "breadcrumbs.enabled": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Hack, Menlo, Monaco, 'Courier New', monospace",
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
}